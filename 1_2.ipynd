def main():
  chislo =float(input())
  if -2**7<=chislo<=+2**7:
    i=chislo
    rez=0
    while i>0:
      a = i % 10  # находим остаток - последнюю цифру  
      i = i // 10   # делим нацело - удаляем последнюю цифру 
      rez = rez * 10    # увеличиваем разрядность второго числа
      rez = rez + a # добавляем очередную цифру
  
    print(rez)
  else:
    print ("no solution")

if __name__=="__main__":
  main()